{

  "FileChecker configs": 
  {
    "PrefixCheckerConfig": 
    {
      "Prefix": "Prefix_", // can't be "", put null
      "MoveWrongFiles": true,
      "PathToMoveDirectory": "/Bin/"
    },

    "ExtensionCheckerConfig": 
    {
    }
  },

  "Directories Arborescence":
  {
    "FileChecker": 
    {
      "ExcludeFromPrefixChecker": true,
      "ExcludeFromExtensionChecker": true
    },

    "Bin": 
    {
      "ExcludeFromPrefixChecker": true,
      "ExcludeFromExtensionChecker": true
    }
  }
}

